{
  "manifest_version": 2,
  "name": "Sonic Player",
  "description": "for listening to and controlling playback of music servers supporting the subsonic API.",
  "homepage_url": "https://github.com/sunikkupfer/sonicplayer",
  "version": "0.0.1",
  "icons": {
    "64": "img/icon.png"
  },
  "permissions": [],
  "background": {
    "scripts": [
      "jslib/browser-polyfill-0.7.0.min.js",
      "background_script.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "64": "img/icon.png"
    },
    "default_title": "Sonic Player",
    "default_popup": "browserAction/index.html"
  },
  "options_ui": {
    "page": "options/index.html"
  }
}
